<div class="feed-list feed-number-<%= @portlet.id %>">
  <h1><%= @feed.title %></h1>
  <ul>
    <% @items.each do |item| %>
      <li>
        <div class="title"><%= link_to item.title, item.link %></div>
        <div class="description"><%= item.description %></div>
        <div class="item-meta">by <%= item.dc_creator %> on <%= item.pubDate %></div>
      </li>
    <% end %>
  </ul>
</div>
